{
	"presets": [
		"env",
		"stage-0"
	],
	"plugins": [
		[
			"syntax-dynamic-import"
		],
		[
			"transform-decorators-legacy"
		],
		[
			"transform-vue-jsx"
		],
		[
			"transform-runtime", 
			{
				"polyfill": false,
				"regenerator": true
			}
		],
		[
			"import",
			[
				{
					"libraryName": "iview",
					"libraryDirectory": "src/components"
				}
			]
		]
	]
}
